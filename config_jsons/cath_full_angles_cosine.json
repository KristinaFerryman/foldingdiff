{
    "angles_definitions": "canonical-full-angles",
    "timesteps": 1000,
    "max_seq_len": 256,
    "min_seq_len": 40,
    "trim_strategy": "randomcrop",
    "variance_schedule": "cosine",
    "variance_scale": 1.0,
    "time_encoding": "gaussian_fourier",
    "position_embedding_type": "relative_key",
    "num_hidden_layers": 12,
    "hidden_size": 384,
    "intermediate_size": 768,
    "num_heads": 12,
    "dropout_p": 0.1,
    "decoder": "mlp",
    "gradient_clip": 1.0,
    "lr": 5e-5,
    "loss": "smooth_l1",
    "sequence_loss_weight": 1.0,
    "l2_norm": 0.0,
    "l1_norm": 0.0,
    "circle_reg": 0.0,
    "lr_scheduler": "LinearWarmup",
    "min_epochs": 100,
    "max_epochs": 100,
    "early_stop_patience": 0,
    "use_swa": false,
    "batch_size": 64,
    "multithread": true
}
